source 'https://rubygems.org'

gem 'jekyll', '~> 4.1.1'

group :jekyll_plugins do
  gem 'jekyll-admin'
  gem 'jekyll-babel'
  gem 'jekyll-feed', '~> 0.12'
  gem 'jekyll-seo-tag'
end

platforms :mingw, :x64_mingw, :mswin, :jruby do
  gem 'tzinfo', '~> 1.2'
  gem 'tzinfo-data'
end

gem 'wdm', '~> 0.1.1', platforms: %i[mingw x64_mingw mswin]
